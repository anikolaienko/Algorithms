echo "Executing $1"

g++ -arch x86_64            \
    -std=c++17              \
    -Wl,-rpath,./math/lib   \
    -Imath/include          \
    -Ldata_structs/lib      \
    -Idata_structs/include  \
    -o out_tmp.o $1         \
    ./math/lib/libmath.dylib

if [ $? -eq 0 ]
then
    ./out_tmp.o
    rm ./out_tmp.o
else
    echo "Error"
fi
